{
    "name": "splitse_app",
    "private": true,
    "description": "splitwise tools for us",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "clean": "rm -rf ./node_modules",
        "clean-all": "yarn clean && yarn workspaces run clean",
        "build": "tsc -b",
        "watch": "tsc -b -w",
        "run-all": "yarn clean-all && yarn && tsc -b"
    },
    "author": "Francia F. Riesco",
    "devDependencies": {
        "concurrently": "^5.3.0",
        "lerna": "^3.22.1",
        "typescript": "^4.2.3"
    }
}